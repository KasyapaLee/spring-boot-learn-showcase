<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.dobby.mybatis.mapper.UserMapper">

    <select id="getAll" resultType="com.dobby.mybatis.model.User">
        SELECT id, name, password, age
        FROM t_users
    </select>


    <select id="getByAgeMoreThan" resultType="com.dobby.mybatis.model.User">
        select id, name, password, age
        from t_users
        where age > #{age}
    </select>


</mapper>